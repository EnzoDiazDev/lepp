{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "strict": true,
      "noImplicitAny": false,
      "outDir": "./dist/",
      "baseUrl": "./src/",
      "experimentalDecorators": true
    },
    "exclude": [
      "./node_modules/",
      "./dist/",
    ],
    "include": [
      "./src/",
    ],
}